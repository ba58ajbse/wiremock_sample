{
  "request": {
    "method": "POST",
    "url": "/api/login",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.username",
          "equalTo": "admin"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "meta": {},
      "data": {
        "message": "Welcome, admin!"
      }
    }
  }
}